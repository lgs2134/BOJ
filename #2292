import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int input=sc.nextInt();
		sc.close();
		int result=0;	
		int n=1;		
		int a1=2;		
		if(input==1) {
			result=1;
		}
		while(a1<=input) {
			a1+=6*n;
      n++;
		}
		System.out.println(n);
	}
}
