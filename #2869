import java.util.*;
public class Main {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int a=sc.nextInt();
		int b=sc.nextInt();
		int v=sc.nextInt();
		sc.close();
		double day=(double)(v-b)/(double)(a-b);
        int day1=(int)Math.ceil(day);
		System.out.println(day1);
	}
}
