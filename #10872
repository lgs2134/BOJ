import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		System.out.println(factorial(n));

	}
	
	public static int factorial(int n) {
		int result=n;
		if(n==0) {
			result=1;
		}
		else {
			result*=factorial(n-1);
		}
		return result;
	}

}
